{
  "version": "2.0.0",
  "plugin": "supernova",
  "template_type": "task_list",
  "description": "JSON task list format for autonomous mode",

  "tasks": [
    {
      "id": 1,
      "title": "Setup project structure",
      "description": "Create folder structure and initial files",
      "acceptance_criteria": [
        "src/ folder exists",
        "tests/ folder exists",
        "package.json has correct metadata"
      ],
      "files_to_modify": [
        "package.json",
        "src/index.js"
      ],
      "dependencies": [],
      "estimated_complexity": "low"
    },
    {
      "id": 2,
      "title": "Implement core functionality",
      "description": "Build the main feature",
      "acceptance_criteria": [
        "All functions implemented",
        "Unit tests pass",
        "No lint errors"
      ],
      "files_to_modify": [
        "src/core.js",
        "tests/core.test.js"
      ],
      "dependencies": [1],
      "estimated_complexity": "medium"
    },
    {
      "id": 3,
      "title": "Add API endpoints",
      "description": "Create REST endpoints",
      "acceptance_criteria": [
        "GET /api/resource works",
        "POST /api/resource works",
        "Error handling implemented"
      ],
      "files_to_modify": [
        "src/routes.js",
        "src/server.js"
      ],
      "dependencies": [2],
      "estimated_complexity": "medium"
    }
  ],

  "quality_gates": {
    "debug_agent": {
      "min_slop_score": 7
    },
    "code_review_agent": {
      "verdict": "PASS"
    },
    "security_agent": {
      "max_severity": "medium"
    }
  },

  "metadata": {
    "project_name": "Example Project",
    "language": "javascript",
    "framework": "express"
  }
}
